}

<script defer>
    console.log("jeg er et script og jeg kører");
    console.log("Username er: " + '@TempData["username"]');
    // man er nødt til at checke om det er null i c#, da det giver syntax error hvis man gør det i JS 
    @if (TempData["username"] != null)
    {
        @: console.log("name recieved, saving cookies");
        @: document.cookie = "username=@TempData["username"]; path=/";
        @: document.cookie = "password=@TempData["password"]; path=/";
